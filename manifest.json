{
    "name": "Email Subscription Form",
    "version": "1.0",
    "description": "A chrome extension that display an email subscription form.",
    "permissions": [
      "activeTab",
      "https://mailchimp.us4.list-manage.com/*"
    ],
    "content_scripts": [
      {
        "matches": [
          "https://mailchimp.com/*"
        ],
        "js": ["jquery.js", "node_modules/easy-autocomplete/dist/jquery.easy-autocomplete.min.js", "script.js"], 
        "css": ["node_modules/easy-autocomplete/dist/easy-autocomplete.min.css"]
      }
    ],
    "background": { "scripts": ["hot-reload.js"] },
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "manifest_version": 2,
    "web_accessible_resources": [
      "images/signup-pencil.png",
      "node_modules/easy-autocomplete/dist/easy-autocomplete.min.css"
    ]
  }
  
  
  